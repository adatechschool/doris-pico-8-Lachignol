-- interactions

function interact()

	if (dir_vector_x) and (dir_vector_y) then
		object_x = (perso.x + dir_vector_x)
		object_y = (perso.y + dir_vector_y)
	else 
		object_x = (perso.x + 1)
		object_y = (perso.y - 1)
	end

	if (btnp(e)) then
		if (check_flag(2,(object_x),(object_y)))
			then
			mget(object_x, object_y)
			fset(sprite, 0, false)
		end
	end
end


function print_outline(text, x, y)
	print(text, x-1, y, 0)
	print(text, x+1, y, 0)
	print(text, x, y-1, 0)
	print(text, x, y+1, 0)
	print(text, x, y, 7)
end
