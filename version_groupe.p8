-- sequence principale

-- au lancement
function _init()
	create_player()
	init_projectiles()
	p = make_ennemi(60,120,1)
 	p.cooldown = 0
 	p.tag = 1
 	p.box = {x1=1,y1=1,x2=6,y2=6}
 	for i =1,10 do
 	mstr = make_ennemi(rnd(128),rnd(48),192)
 	mstr.dy =1
 	mstr.tag = 2
	end
end
 
-- mise a jour a chaque frame (60 fois par secondes)
-- mise a jour a chaque frame (60 fois par secondes)
function _update60()
	player_movement()
	get_vecteur()
	updt_projectiles()
	update_camera()
	move_ennemis()
	collisions()
end

-- affichage des sprites
function _draw()

	-- clear interface
	cls()

	-- draw map on "left" corner
	draw_map()

	-- draw sprit number 1 at xy point
	affichage_perso()

	draw_ennemis()
		print(#ennemis,4,64,8)
	end

	draw_projectiles()
	draw_ui()
end



