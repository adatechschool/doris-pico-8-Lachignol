--sれたquence principale

-- au lancement
function _init()
create_player()
init_projectiles()
end
 
-- mise a jour a chaque frame (60 fois par secondes)
function _update60()
player_movement()
interact()
get_speed()
updt_projectiles()
end

-- affichage des sprites
function _draw()

--clear interface
cls()
--draw map on "left" corner
map(0,0,0,0)
--draw sprit number 1 at xy point
affichage_perso()



-- coin superieur g. camera
cam_x=0
cam_y=0

draw_projectiles()

end



