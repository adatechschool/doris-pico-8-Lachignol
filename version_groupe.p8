-- interactions

function interact()

	local object_x = (neox+speedx)
	local object_y = (neoy+speedy)

	if (btnp(e)) then
		if (checkflag(2,(object_x),(object_y)))	then 
			fset(sprite, 0, false)
				 end
	end
end



